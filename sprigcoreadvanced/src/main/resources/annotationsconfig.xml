    <context:annotation-config />

    <bean name="address123" class="br.com.brunomilitzer.trainings.spring.springjdbc.sprigcoreadvanced.annoations.Address" p:number="123" p:street="Rua Fernando Ferreira de Mello" p:city="Florianópolis" />

    <bean name="address456" class="br.com.brunomilitzer.trainings.spring.springjdbc.sprigcoreadvanced.annoations.Address" p:number="456" p:street="Rua Fernando Ferreira de Mello" p:city="Florianópolis" />

